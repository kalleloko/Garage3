@model Garage3.Models.VehicleDetailViewModel

@{
    ViewData["Title"] = "Delete";
}

<h3>Are you sure you want to pick up this vehicle?</h3>

<partial name="_VehicleDetailPartial" model="Model" />
    
<form class="my-3" asp-action="Delete">
    <input type="hidden" asp-for="Id" />
    <input  type="submit" value="Pick up" class="btn btn-secondary" />
    <a asp-action="HomePage" class="btn btn-primary"> Cancel </a>
</form>
